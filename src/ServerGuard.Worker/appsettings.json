{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "RabbitMq": "amqp://guest:guest@serverguard.rabbtimq",
    "Timescale": "Host=serverguard.timescale;Port=5432;Username=postgres;Password=mysecretpassword;Database=serverguard-db;Include Error Detail=true;",
    "Cosmos": "AccountEndpoint=https://serverguard-cosmos.documents.azure.com:443/;AccountKey=KdqsXfAfPy58g8PSeeV2oAPYOIoMMRAHMhKvEI9rjpYFvL5qxXDcgawMcRtdZakFRoVAyw6sctB7ACDbRU3gQA==;",
    "ClickHouse": "Host=v96gqkfeks.eu-central-1.aws.clickhouse.cloud;Port=8443;Protocol=https;Username=default;Password=kgr1HxtJ1TF~L;Database=default"
  },
  "UsePostgres": false,
  "UseCosmos": false
}
